{
  "title": "SCCM Unified - System & Role Health",
  "description": "CPU, Memory, Disk, Network, Role Service Health, and Role Events. Filter by application and domain.",
  "layout_type": "ordered",
  "reflow_type": "fixed",
  "template_variables": [
    {
      "name": "applicationid",
      "prefix": "applicationid",
      "available_values": ["SYS-1234"],
      "default": "SYS-1234"
    },
    {
      "name": "domain",
      "prefix": "domain",
      "available_values": ["Corp", "QA"],
      "default": "Corp"
    }
  ],
  "widgets": [
    {
      "definition": {
        "type": "note",
        "content": "# CPU, Memory, Disk, Network",
        "background_color": "green",
        "font_size": "14",
        "text_align": "left",
        "vertical_align": "top",
        "show_tick": false,
        "has_padding": true
      },
      "layout": { "x": 0, "y": 0, "width": 12, "height": 2 }
    },
    {
      "definition": {
        "title": "CPU (system %)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [{ "formula": "query2" }],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query2",
                "query": "avg:system.cpu.system{$applicationid,$domain} by {host}"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "dog_classic",
              "order_by": "values",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "line"
          }
        ],
        "markers": [
          { "label": " 95% ", "value": "y = 95", "display_type": "error dashed" },
          { "label": " 85% ", "value": "y = 85", "display_type": "warning dashed" }
        ]
      },
      "layout": { "x": 0, "y": 2, "width": 6, "height": 3 }
    },
    {
      "definition": {
        "title": "Memory (used %)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [{ "formula": "1 - query2" }],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query2",
                "query": "avg:system.mem.pct_usable{$applicationid,$domain} by {host}"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "dog_classic",
              "order_by": "values",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "line"
          }
        ],
        "markers": [
          { "label": " 95% ", "value": "y = 0.95", "display_type": "error dashed" },
          { "label": " 85% ", "value": "y = 0.85", "display_type": "warning dashed" }
        ]
      },
      "layout": { "x": 6, "y": 2, "width": 6, "height": 3 }
    },
    {
      "definition": {
        "title": "Disk C: usage (%) - SCCM Servers",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value", "sum"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [{ "formula": "query2 / query1 * 100" }],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query2",
                "query": "avg:system.disk.used{$applicationid,$domain,sccmrole:sccm-svr,device:c:} by {host}"
              },
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "avg:system.disk.total{$applicationid,$domain,sccmrole:sccm-svr,device:c:} by {host}"
              }
            ],
            "response_format": "timeseries",
            "style": { "palette": "dog_classic", "order_by": "values", "line_type": "solid", "line_width": "normal" },
            "display_type": "line"
          }
        ],
        "markers": [
          { "label": " 95% ", "value": "y = 95", "display_type": "error dashed" },
          { "label": " 85% ", "value": "y = 85", "display_type": "warning dashed" }
        ]
      },
      "layout": { "x": 0, "y": 5, "width": 3, "height": 3 }
    },
    {
      "definition": {
        "title": "Disk E: usage (%) - SCCM Servers",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
