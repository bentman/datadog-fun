    {
      "id": 12,
      "definition": {
        "title": "Network (Mb/s)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "horizontal",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [
              {
                "alias": "Bytes sent",
                "number_format": {
                  "unit": {
                    "type": "canonical_unit",
                    "unit_name": "megabit",
                    "per_unit_name": "second"
                  }
                },
                "formula": "query1"
              }
            ],
            "response_format": "timeseries",
            "queries": [
              {
                "query": "sum:system.net.bytes_sent{environment:$environment} by {host}.weighted()",
                "data_source": "metrics",
                "name": "query1"
              }
            ],
            "style": { "palette": "dog_classic", "line_type": "solid", "line_width": "thin" },
            "display_type": "line"
          },
          {
            "formulas": [
              {
                "alias": "Bytes received",
                "number_format": {
                  "unit": {
                    "type": "canonical_unit",
                    "unit_name": "megabit",
                    "per_unit_name": "second"
                  }
                },
                "formula": "0 - query1"
              }
            ],
            "response_format": "timeseries",
            "queries": [
              {
                "query": "sum:system.net.bytes_rcvd{environment:$environment} by {host}.weighted()",
