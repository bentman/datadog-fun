              }
            ],
            "style": { "palette": "dog_classic", "line_type": "solid", "line_width": "thin" },
            "display_type": "line"
          }
        ],
        "yaxis": { "include_zero": true, "scale": "linear", "min": "auto", "max": "auto" }
