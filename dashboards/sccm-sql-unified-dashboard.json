        ],
        "markers": [
          { "label": " 95% ", "value": "y = 0.95", "display_type": "error dashed" },
          { "label": " 85% ", "value": "y = 0.85", "display_type": "warning dashed" }
        ]
      },
      "layout": { "x": 6, "y": 2, "width": 6, "height": 3 }
    },

