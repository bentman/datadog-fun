{
  "title": "SCCM Unified - System & Role Health",
  "description": "CPU, Memory, Disk, Network, Role Service Health, and Role Events. Filter by environment.",
  "layout_type": "ordered",
  "reflow_type": "fixed",
  "template_variables": [
    {
      "name": "environment",
      "prefix": "environment",
      "available_values": ["production", "qa"],
      "default": "production"
    }
  ],
  "widgets": [
    {
      "id": 0,
      "definition": {
        "type": "note",
        "content": "# CPU, Memory, Disk, Network",
        "background_color": "green",
        "font_size": "14",
        "text_align": "left",
        "vertical_align": "top",
        "show_tick": false,
        "has_padding": true
      },
      "layout": { "x": 0, "y": 0, "width": 12, "height": 2 }
    },

    {
      "id": 1,
      "definition": {
        "title": "CPU (system %)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [{ "formula": "query2" }],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query2",
                "query": "avg:system.cpu.system{environment:$environment} by {host}"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "dog_classic",
              "order_by": "values",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "line"
          }
        ],
        "markers": [
          { "label": " 95% ", "value": "y = 95", "display_type": "error dashed" },
          { "label": " 85% ", "value": "y = 85", "display_type": "warning dashed" }
        ]
      },
      "layout": { "x": 0, "y": 2, "width": 6, "height": 3 }
    },
    {
      "id": 2,
      "definition": {
        "title": "Memory (used %)",
        "title_size": "16",
        "title_align": "left",
        "show_legend": true,
        "legend_layout": "auto",
        "legend_columns": ["avg", "min", "max", "value"],
        "type": "timeseries",
        "requests": [
          {
            "formulas": [{ "formula": "1 - query2" }],
            "queries": [
              {
                "data_source": "metrics",
                "name": "query2",
                "query": "avg:system.mem.pct_usable{environment:$environment} by {host}"
              }
            ],
            "response_format": "timeseries",
            "style": {
              "palette": "dog_classic",
              "order_by": "values",
              "line_type": "solid",
              "line_width": "normal"
            },
            "display_type": "line"
